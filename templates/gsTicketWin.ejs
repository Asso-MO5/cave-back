<style>
    * {
        box-sizing: border-box;
        font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    }

    .title {
        background-color: #4088cf;
        color: #FFF;
        padding: 15px;
        font-size: 27px;
        position: fixed;
        top: .8cm;
        left: 1cm;
        width: 13cm;
        height: 5cm;
        vertical-align: middle;
        font-weight: bold;

    }

    .mo5 {
        position: fixed;
        top: 4.4cm;
        right: 7.3cm;
        width: 4.5cm;
    }


    .noMo5 {
        font-size: 24px;
    }

    .poster {
        position: fixed;
        top: 6.6cm;
        left: 1cm;
        width: 13cm;
    }

    .qr {
        position: fixed;
        top: .8cm;
        right: .75cm;
        width: 5cm;
        height: 5cm;
    }

    .logos {
        position: fixed;
        top: 6.5cm;
        right: 2.6cm;
    }

    .logo {
        display: block;
        margin-top: 1.08cm;
        width: 1.6cm;
        height: 1.6cm;
    }

    img {
        width: 100%;
        height: auto;
    }

    .qrLink {
        position: fixed;
        top: 6.2cm;
        right: .75cm;
        width: 5cm;
        height: 1cm;
        display: block;
        text-align: center;
        color: #3490dc;
    }

    .tipeee {
        bottom: .8cm;
        left: 1cm;
        right: 1cm;
        height: 2cm;
        position: fixed;
        padding: 0.3cm;
        background: #d84759;
        color: white;
        vertical-align: middle;
        border-radius: .3cm;
    }

    .tipeee a,
    .tipeee p {
        color: white !important;
        font-weight: bold;
        font-size: 1.3rem;
        margin-right: 3cm;
    }

    .tipeee img {
        position: fixed;
        pointer-events: none;
        bottom: 1.3cm;
        right: 1cm;
        width: 3.5cm;
        height: auto;
        margin-right: .3cm;
    }

    a {
        text-decoration: none;
        color: #3490dc !important
    }
</style>

<body style="background-color: #FFF; color: #000; font-family: Arial, sans-serif; margin: 1cm; padding: 0;">

    <div class="title <%= noMo5 %>">
        <%= title %>
    </div>
    <img src="<%= mo5Logo %>" alt="affiche game story" class="mo5" />

    <img src="<%= poster %>" alt="affiche game story" class="poster" />

    <img src="<%= img %>" alt="QR CODE de votre place" class="qr" />

    <a href="<%= url %>" alt="lien pour acceder à votre place" class="qrLink">
        ou cliquez ici
    </a>

    <a href="https://fr.tipeee.com/association-mo5-com" target="_blank" alt="lien pour acceder à votre place"
        class="tipeee">
        <table>
            <tr>

                <td>
                    <p>
                        Soutenez la création d'un musée vivant des cultures numériques, cliquez ici
                    </p>
                </td>
                <td>
                    <img src="<%= tipeee %>" alt="tipeee" />

                </td>
            </tr>
        </table>
    </a>



    <div class="logos">
        <% logos.forEach(function(logo) { %>
            <a class="logo" href="<%= logo.url %>" alt="logo.name" target="_blank">
                <img src="<%= logo.img %>" alt="logo.name" />
            </a>
            <% }); %>
    </div>



</body>